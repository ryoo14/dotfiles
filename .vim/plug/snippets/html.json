{
  "html": {
    "prefix": "html",
    "body": [
      "<!DOCTYPE html>",
      "<html lang=\"en\">",
      "  <head>",
      "    <meta charset=\"UTF-8\">",
      "    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
      "    <link rel=\"stylesheet\" href=\"${1:link}\">",
      "    <title>${2:title}</title>",
      "  </head>",
      "  <body>",
      "    ${3:body}",
      "  </body>",
      "</html>"
    ],
    "description": "html base",
    "scope": "source.html"
  },
  "div": {
    "prefix": "div",
    "body": "<div class=\"${1:class}\">${2:content}</div>",
    "description": "div class",
    "scope": "source.html"
  },
  "script": {
    "prefix": "script",
    "body": "<script src=\"${1:src}\"></script>${2:eol}",
    "description": "script",
    "scope": "source.html"
  }

}
